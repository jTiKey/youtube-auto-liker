{
  "manifest_version": 3,
  "name": "YouTube Like Button Detector",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "webNavigation"],
  "host_permissions": ["https://www.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}